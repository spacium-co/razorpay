{
    "name": "spaciumco/razorpay",
    "description": "Razorpay payment integration for laravel",
    "keywords": [
        "Razorpay", "Laravel", "composer"
    ],
    "homepage" : "https://github.com/spacium-co/razorpay",
    "support" : {
        "source" : "https://github.com/spacium-co/razorpay",
        "issues" : "https://github.com/spacium-co/razorpay/issues"
    },
    "require": {
        "razorpay/razorpay": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "spacium",
            "email": "spaciuminfotech@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Spaciumco\\Razorpay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spaciumco\\Razorpay\\Providers\\RazorpayServiceProvider",
                "Spaciumco\\Razorpay\\Providers\\PaymentServiceProvider"
            ],
            "aliases": {
                "Razorpay": "Spacium-co\\Razorpay\\Razorpay"
            }
        }
    }
}
